pipeline {
    agent any
    triggers {
        genericWebhooktriggers(
            genericVariables: [
                [key: 'token', value: 'mytoken']
            ],
            token: 'mytoken'
        )
    }
    stages {
        stage('Stage-1') {
              steps {
                sh 'touch tomcat'
            }
        }

        stage('Stage-2') {
            steps {
                sh 'touch master'
            }
        }
        stage('Stage-3') {
            steps {
                sh 'touch file1'
    }
}
stage('Stage-4') {
            steps {
                sh 'touch file2'
    }
}
}
}
